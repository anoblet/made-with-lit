{
    "name": "@anoblet/made-with-lit",
    "version": "0.3.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "rimraf public/scripts && rollup -c etc/rollup.production.js",
        "build:watch": "rollup -c etc/rollup.development.js -w",
        "commit": "git-cz",
        "dev": "npm-run-all -p build:watch start",
        "publish": "npm run build && npm run service-worker && firebase deploy",
        "save": "git add . && git commit -m \"chore(app): save\"",
        "service-worker": "node bin/workbox-build.js && rollup -c etc/rollup.service-worker.js && rimraf tmp",
        "start": "node bin/server.js",
        "test": "karma start --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anoblet/made-with-lit.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/anoblet/made-with-lit/issues"
    },
    "homepage": "https://github.com/anoblet/made-with-lit#readme",
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@open-wc/testing": "^2.5.7",
        "@open-wc/testing-karma": "^3.3.8",
        "@rollup/plugin-json": "^4.0.3",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@rollup/plugin-replace": "^2.3.1",
        "@rollup/plugin-typescript": "^3.0.0",
        "browser-sync": "^2.26.7",
        "commitizen": "^4.0.3",
        "compression": "^1.7.4",
        "connect-history-api-fallback": "^1.6.0",
        "cz-conventional-changelog": "^3.1.0",
        "deepmerge": "^4.2.2",
        "karma": "^5.2.2",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "rollup": "^1.31.1",
        "rollup-plugin-minify-html-literals": "^1.2.3",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-size": "^0.2.2",
        "rollup-plugin-terser": "^5.2.0",
        "workbox-build": "^5.0.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@anoblet/firebase": "git+https://github.com/anoblet/firebase.git",
